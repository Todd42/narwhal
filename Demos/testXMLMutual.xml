<?xml version="1.0"?>
<parser>
    <argument type="positional">
        <name>pet</name>
        <datatype>String</datatype>
        <description>Its a pet</description>
        <restrictions>
            <restrict>dog</restrict>
            <restrict>cat</restrict>
            <restrict>pig</restrict>
        </restrictions>
        <nickname></nickname>
        <arity>1</arity>
    </argument>
    
    <argument type="positional">
        <name>rainy</name>
        <datatype>boolean</datatype>
        <description>Is it rainy</description>
        <nickname></nickname>
        <restrictions></restrictions>
        <arity>1</arity>
    </argument>
    
    <argument type="positional">
        <name>bathrooms</name>
        <datatype>float</datatype>
        <description></description>
        <nickname></nickname>
        <restrictions></restrictions>
        <arity>1</arity>
    </argument>
    
    <argument type="named">
        <name>Gender</name>
        <default></default>
        <datatype>String</datatype>
        <nickname></nickname>
        <required></required>
        <restrictions></restrictions>
    </argument>
    
    <argument type="named">
        <name>Height</name>
        <default></default>
        <datatype>int</datatype>
        <nickname>h</nickname>
        <required></required>
        <restrictions></restrictions>
    </argument>
    
    <argument type="named">
        <name>Color</name>
        <default></default>
        <datatype>String</datatype>
        <nickname>c</nickname>
        <required></required>
        <restrictions>
            <restrict>red</restrict>
            <restrict>green</restrict>
            <restrict>blue</restrict>
        </restrictions>
    </argument>
    
    <argument type="named">
        <name>Shape</name>
        <default></default>
        <datatype>String</datatype>
        <nickname>s</nickname>
        <required></required>
    </argument>
    
    <argument type="flag">
        <flagname>x</flagname>
    </argument>
    
    <argument type="flag">
        <flagname>w</flagname>
    </argument>
    <groups>
        <group>
            <args>Gender</args>
            <args>Height</args>
        </group>
        <group>
            <args>Color</args>
            <args>Shape</args>
        </group>
    </groups>
</parser>